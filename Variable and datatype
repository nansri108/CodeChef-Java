class Codechef
{
	public static void main (String[] args)
	{
		// your code goes here
		int a = -50;
		int b = 40;
		int sum = 40+(-50);
		int product = -50*40;
		int quotient = -50/40;
		System.out.println(sum);
		System.out.println(product);	
		System.out.print(quotient);	
	}
}
