class Codechef
{
	public static void main (String[] args)
	{
		// Declare 'a' as a char and assign it the value C
		char a = 'C';
		
		// Declare 'b' as a String and assign it the value Chef
		String b ="Chef";
		System.out.print(a);
		System.out.print(b);
	}
}
