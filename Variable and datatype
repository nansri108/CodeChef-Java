class Codechef
{
	public static void main (String[] args)
	{
		// your code goes here
		double pi = 3.14;
		double radius = 8.9 ;
		double area = pi * radius *radius;
		System.out.println("The Area of the given Circle is - ");
		System.out.print(area);
        
	}
}
